{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": ["./", "README.md"],
    "includePattern": "\\.js$",
    "excludePattern": "node_modules"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "docdash": {
    "sort": true,
    "search": true,
    "collapse": true
  }
}
